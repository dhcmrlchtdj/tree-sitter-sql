{
    "name": "tree-sitter-sqlite",
    "version": "0.0.1",
    "description": "SQLite grammar for the tree-sitter parsing library",
    "author": "H11",
    "license": "MIT",
    "dependencies": {
        "nan": "2.14.2"
    },
    "devDependencies": {
        "prettier": "2.3.0",
        "tree-sitter-cli": "0.20.0"
    },
    "tree-sitter": [
        {
            "scope": "source.sql",
            "file-types": ["sql"],
            "highlights": "queries/highlights.scm",
            "injection-regex": "^(sql)$"
        }
    ],
    "main": "bindings/node"
}
